			   /*----- about style start ----------*/

.about-section{
	position:relative;
	z-index:1;
	padding:60px 0;
	
}

.about-section:after{
	content: "";
	position:absolute;
	left:0;
	top:0;
	width:100%;
	height:100%;
	z-index:-1;
	transform:skew(0deg, -2.5deg);
	background-color:#000;
	
}
.about-content{
	color:#fff;	
	padding-top:40px;
}
.about-content h1{
	font-size:35px;
	font-weight:700;
	padding-bottom:40px;
}
.about-content p{
	font-size:16px;
	font-weight:500;
	padding-bottom:40px;
}
.about-content a{
	text-decoration:none;
	font-weight:500;
	font-size:13px;
	color:#fff;
	border:2px solid #e03a3c;
	padding:8px 33px;
	border-radius:20px;
	display:inline-block;
}
.about-content a:hover{
	background-color:#e03a3c;
}
.about-content a i{
	font-size:13px;
	font-weight:500;
	padding-left:15px;
}

/*----list box---*/
.about-section .list-box{
	margin-bottom:30px;
	
}
.about-section .list-box i{
	font-size:40px;
	color:#e03a3c;
}
.about-section .list-box h4{
	color:#fff;
	font-size:20px;
	font-weight:700;
	margin-top:10px;
}
.about-section .list-box p{
	color:#848484;
	font-size:15px;
	margin-top:10px;
	padding-bottom:30px;
}

			   
