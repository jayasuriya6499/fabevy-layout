             /*----- header menu style start ----------*/

.header-menu{
	background-color:#fff;
	padding:18px 0;
}
.logo a{
	text-decoration:none;
	color:#000;
	font-size:30px;
	font-weight:700;
	display:inline-block;
}
.logo span{
	color:#e03a3c;
}

.navbar .navbar-list li{
	display:inline-block;
	
}
.navbar .navbar-list{
	text-align:right;
}
.navbar-list li a.nav-list{
	display:block;
	text-decoration:none;
	color:#000;
	padding-left:14px;
	padding-right:14px;
	transition:all 0.3s ease;
	font-size:15px;
	padding:10px 14px;
}
.navbar-list li a.nav-list:hover{
	color:#e03a3c;
}
.nav-btn{
	font-size:14px;
}
.dropdown{
	position:relative;
}
.dropdown:hover .dropdown-nav{
	transform:translateY(0);
	opacity:1;
	visibility:visible;
}
.dropdown-nav{
	position:absolute;
	top:100;
	left:0;
	min-width:190px;
	background-color:#fff;
	box-shadow:2px 2px 18px -5px #ccc;
	border-radius:2px;
	text-align:left;
	transform:translateY(20px);
	opacity:0;
	visibility:hidden;
	transition:all 0.3s ease;
}
.dropdown-nav li{
	display:block;
}
.dropdown-nav li a{
	color:#000;
	font-size:14px;
	display:block;
	text-decoration:none;
	padding:8px 15px;
}
.dropdown-in{
	position:relative;
}
.dropdown-in i{
	padding-left:60px;
}
.sub-dropdown{
	position:absolute;
	top:0;
	left:100%;
	min-width:190px;
	background-color:#fff;
	box-shadow:2px 2px 18px -5px #ccc;
	border-radius:2px;
	text-align:left;
	transform:translateX(-15px);
	opacity:0;
	visibility:hidden;
	transition:all 0.3s ease;
	margin-left:80px;
}
.dropdown  a.nav-list{
	padding-right:25px;
	
}
.dropdown a  i{
	position:absolute;
	top:14px;
	left:100px;	
}
.dropdown-in:hover .sub-dropdown{
	transform:translateX(0);
	opacity:1;
	visibility:visible;
}

        /*----- header menu style end ----------*/ 
		
		
		
		

        /*----- banner style start ----------*/ 
		
