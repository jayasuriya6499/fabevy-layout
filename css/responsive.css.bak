@media (max-width: 576px){
	.d-xs-none{
	display:none !important;
    }
	.d-xs-block{
		display:block !important;
	}
	
   .col-xs-1{
	width:8.33%; 
	padding:0 15px;
	}
	.col-xs-2{
	width:16.33%;
	padding:0 15px;
	}
	.col-xs-3{
	width:25%;
	padding:0 15px;
	}
	.col-xs-4{
	width:33.33%;
	padding:0 15px;
	}
	.col-xs-5{
	width:41.66%;
	padding:0 15px;
	}
	.col-xs-6{
	width:50%;
	padding:0 15px;
	}
	.col-xs-7{
	width:58.33%;
	padding:0 15px;
	}
	.col-xs-8{
	width:66.66%;
	padding:0 15px;
	}
	.col-xs-9{
	width:75%;
	padding:0 15px;
	}
	.col-xs-10{
	width:83.33%;
	padding:0 15px;
	}
	.col-xs-11{
	width:91.66%;
	padding:0 15px;
	}
	.col-xs-12{
	width:100%;
	padding:0 15px;
	}
	
	/*--- logo ---*/
	
	.nav-btn{
		font-size:10px;
		margin-top:10px;
		    padding-left: 38px;
		
		
	}
	
	/*--- banner ---*/
	
	.banner-content{
		text-align:center;
	}
	.banner-content h1{
		font-size: 27px;
		padding-top:50px;
	}
	.banner-content h3{
		font-size:17px;
	}
	
	/*--- about ---*/
	
	.about-section{
		text-align:center;
	}
	.about-content{
		margin-bottom:80px;
	}
	.about-list-box{
		margin-bottom:40px;
	}
	
	
	/*--- achievements ---*/
	
	.events{
		margin-bottom:70px;
	}
	
	
	/*--- content-section ---*/
	
	.content-box-1{
		margin-bottom:30px;
	}
	
	.content-section .main-content h1{
		font-size:22px;
	}
		


    /*---- services ----*/
	
	.services-list-box{
		padding-top:22px;
		padding-bottom:22px;
		padding-left:90px;
		padding-right:10px;
	}
	
	
	/*-- questions ---*/
	
	.qus-line1 h5{
		font-size:16px;
	}
	.qus-line1 i{
		right:-15px;
	}
	
	
	/*---- team ----*/
	
	.team-box{
		margin-bottom:30px;
	}
	
	
	/*--- contact ---*/
	
	.call-box{
		margin-bottom:30px;
	}
	
	
	/*--- footer ----*/
	
	.main-details{
		margin-bottom:40px;
	}
	.useful-links{
		margin-bottom:40px;
	}
	.our-services{
		margin-bottom:40px;
	}
	
	/*---- copyright ---*/
	
	.copyright{
		text-align:center;
	}
	
	.rights-link{
		margin-bottom:20px;
	}
	
	.copyright .end-link{
		text-align:center;
	}
	
	
	
	
	 
	
}

















